{
  "rules": {
    "trailing-comma": [
      true,
      {
        "multiline": "always",
        "singleline": "never"
      }
    ],
    "import-name": false,
    "max-line-length": 120,
    "no-trailing-whitespace": [false],
    "no-var-requires": false,
    "variable-name": [false, "check-format", "allow-leading-underscore"]
  },
  "extends": ["tslint-config-airbnb", "tslint-config-prettier"]
}
