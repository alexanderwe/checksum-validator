{
  "extends": "tslint-config-airbnb",
  "rules": {
    "quotemark": [true, "single"],
    "trailing-comma": [
      true,
      {
        "multiline": "always",
        "singleline": "never"
      }
    ],
    "import-name": false,
    "max-line-length": 200,
    "no-trailing-whitespace": [false],
    "no-var-requires": false,
    "variable-name": [true, "check-format", "allow-leading-underscore"]
  }
}
