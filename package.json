{
  "name": "checksum-validator",
  "version": "0.2.1",
  "description": "Small Electron application to validate checksums.",
  "main": "main.js",
  "scripts": {
    "babel:watch": "babel ./app/main/src/ -w -d ./app/main/build",
    "build:watch": "webpack --env.mode=watch",
    "build": "webpack --env.mode=noWatch",
    "dist": "build -m --x64 --ia32",
    "postinstall": "install-app-deps",
    "release": "build",
    "start": "ELECTRON_DEV=1 electron app/build/main.js",
    "test": "mocha",
    "tsc:watch": "tsc --watch --p tsconfig.json --declaration --target es5"
  },
  "author": "Alexander Wei√ü",
  "repository": "https://github.com/alexanderwe/checksum-validator",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/chai-as-promised": "^7.1.0",
    "@types/classnames": "^2.2.3",
    "@types/mocha": "^2.2.44",
    "@types/react": "^16.0.18",
    "@types/react-dom": "^16.0.2",
    "@types/react-transition-group": "^2.0.6",
    "@types/username": "^1.0.28",
    "awesome-typescript-loader": "^3.2.3",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "copy-webpack-plugin": "^4.2.0",
    "css-loader": "^0.28.7",
    "electron": "^1.8.2-beta.4",
    "electron-builder": "^19.37.2",
    "electron-devtools-installer": "^2.2.3",
    "electron-react-devtools": "^0.4.0",
    "electron-reload": "^1.1.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.9.0",
    "mocha": "^4.0.1",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "spectron": "^3.7.2",
    "style-loader": "^0.19.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "tslint-config-airbnb": "^5.3.0",
    "tslint-config-prettier": "^1.6.0",
    "typescript": "^2.5.3",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dashboard": "^1.0.0",
    "webpack-node-externals": "^1.6.0"
  },
  "build": {
    "appId": "com.github.alexanderwe.checksum-validator",
    "productName": "Checksum Validator",
    "publish": [
      {
        "provider": "github",
        "owner": "alexanderwe",
        "repo": "checksum-validator"
      }
    ],
    "mac": {
      "category": "public.app-category.utilities",
      "type": "distribution"
    }
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.6.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1"
  }
}
